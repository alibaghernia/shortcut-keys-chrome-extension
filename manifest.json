{
    "manifest_version": 3,
    "name": "Shortcut Keys",
    "description": "custome new tab",
    "version": "0.0.1",
    "action": {
        "default_popup": "./popup/index.html"
    },
    "background": {
        "service_worker": "./service_worker.js"
    },
    "commands": {
        "open-chatgpt": {
            "suggested_key": "Alt+C",
            "description": "open chatGPt"
        },
        "open-youtube": {
            "suggested_key": "Alt+Y",
            "description": "open youtube"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["inject.js"],
            "css": ["direction.css"]
        }
    ],
    "host_permissions": ["<all_urls>"]
}